{
  "name": "bolt/rssaggregator",
  "description": "RSS aggregator extension for Bolt",
  "type": "bolt-extension",
  "keywords": [
    "RSS"
  ],
  "require": {
    "bolt/bolt": "^3.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Klier",
      "email": "sebastian@sebastianklier.com"
    },
    {
      "name": "Gawain Lynch",
      "email": "gawain.lynch@gmail.com"
    },
    {
      "name": "Rix Beck",
      "email": "rix@boltcms.hu"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Bolt\\Extension\\Bolt\\RSSAggregator\\": "src"
    }
  },
  "extra": {
    "bolt-assets": "assets",
    "bolt-class": "Bolt\\Extension\\Bolt\\RSSAggregator\\RSSAggregatorExtension",
    "bolt-icon": "icon.png"
  }
}
